{
	"ecmaFeatures": {
		"jsx": true,
		"modules": true
	},
	"env": {
		"browser": true,
		"node": true
	},
	"plugins": ["react"],
	"extends": [
		"airbnb",
		"eslint:recommended",
		"plugin:react/recommended",
		"prettier",
		"prettier/react"
	],
	"rules": {
		"react/jsx-filename-extension": 0,
		"react/prefer-stateless-function": 0
	}
}
